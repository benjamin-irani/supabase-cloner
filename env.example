# Supabase Management API Configuration
NEXT_PUBLIC_SUPABASE_URL=your-supabase-url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# OAuth Configuration for Supabase Management API
SUPABASE_MANAGEMENT_API_URL=https://api.supabase.com/v1
SUPABASE_ACCESS_TOKEN=your-access-token
SUPABASE_OAUTH_CLIENT_ID=your-oauth-client-id
SUPABASE_OAUTH_CLIENT_SECRET=your-oauth-client-secret

# Application Configuration
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-nextauth-secret

# Database Configuration (for application data)
DATABASE_URL=postgresql://user:password@localhost:5432/supaclone

# Redis Configuration (for caching and job queue)
REDIS_URL=redis://localhost:6379

# Encryption Keys
ENCRYPTION_KEY=your-32-character-encryption-key

# Monitoring and Logging
LOG_LEVEL=info
ENABLE_METRICS=true

# Development Settings
NODE_ENV=development
